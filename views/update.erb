<?xml version="1.0" encoding="UTF-8"?>
<MediaContainer friendlyName="myPlex" identifier="com.plexapp.plugins.myplex" size="<%= releases.size %>" title="Updates">
  <% releases.each do |release| %>
    <Release id="<%= release.id%>" version="<%= release.version %>" added="" fixed="" live="true" autoupdate="<%= release.autoupdate %>" createdAt="<%=release.time%>">
      <Package file="<%= release.update_url %>" fileHash="<%= release.update_sum %>" fileName="<%= release.update_url.split("/")[-1]  %>" delta="false"/>
    </Release>
  <% end %>
</MediaContainer>
